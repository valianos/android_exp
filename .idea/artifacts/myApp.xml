<component name="ArtifactManager">
  <artifact type="apk" build-on-make="true" name="myApp">
    <output-path>$PROJECT_DIR$/build/classes/artifacts/myApp</output-path>
    <properties id="android-properties">
      <options>
        <option name="keyStoreUrl" value="file://" />
        <option name="proGuardCfgFiles">
          <list>
            <option value="file://%MODULE_SDK_HOME%/tools/proguard/proguard-android.txt" />
          </list>
        </option>
      </options>
    </properties>
    <root id="archive" name="myApp.apk">
      <element id="android-final-package" facet="app/android/Android" />
    </root>
  </artifact>
</component>